    "cookie-parser": "~1.4.3",
    "crypto": "0.0.3",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "es6-promisify": "^5.0.0",
    "express": "~4.15.2",
    "express-session": "^1.15.4",
    "md5": "^2.2.1",
    "mongoose": "^4.11.4",
    "mongoose-mongodb-errors": "0.0.2",
    "morgan": "~1.8.1",
    "node-sass": "^4.5.3",
    "node-sass-middleware": "^0.11.0",
    "passport": "^0.3.2",
    "passport-local-mongoose": "^4.0.0",
    "pug": "~2.0.0-beta11",
